
public class CallByValue {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number=1;
		System.out.println(number);
		System.out.print(incrementByTen(number));
		System.out.println(number);

	}
	public static int incrementByTen(int x){
		x+=10;
		return x;
	}

}
