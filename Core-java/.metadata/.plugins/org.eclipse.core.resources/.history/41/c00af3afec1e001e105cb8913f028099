package com.aurionpro.models;

public class Rectangle {
	public double height;
	public double width;

	public double getHeight() {
		return height;
	}

	public double getWidth() {
		return width;
	}

	public double areaRectangle() {
		int height = 80;
		return this.height * this.width; // this will point to objects variable's value
		// return height*this.width; this will point to local variable's value
	}
}
